  },
  "homepage": "https://github.com/grundstein/plyo#readme",
  "dependencies": {
    "@magic/cli": "0.0.46",
    "@magic/fs": "0.0.28",
    "@magic/types": "0.1.23"
  },
  "devDependencies": {
    "@magic/format": "0.0.51",
    "@magic/test": "0.2.17"
  },
  "contributors": [
    {
      "name": "jascha ehrenreich",
