{
  "name": "plyo",
  "version": "0.0.1",
  "description": "optimize ply files, reduce x,y,z and nx, ny, nz precision, merge duplicate vertices, remove normals, remove alpha",
  "bin": {
    "plyo": "src/bin.js"
  },
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "dev": "./src/bin.js -i docs --remove-normals",
    "format": "f -w",
    "test": "t"
  },
